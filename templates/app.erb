# <%= @name %>
#
# This uwsgi configuration is managed by Puppet.
# Do not edit this file. Your changes will be lost.
[uwsgi]
vhost = <%= @vhost %>
plugins = <%= @plugins %>
<% if @socket -%>
socket = <%= @socket %>
<% end -%>
<% if @http_socket -%>
http-socket = <%= @http_socket %>
<% end -%>
<% if @master -%>
master = <%= @master %>
<% end -%>
<% if @enable_threads -%>
enable-threads = <%= @enable_threads %>
<% end -%>
processes = <%= @processes %>
<% if @env -%>
env = <%= @env %>
<% end -%>
<% if @pythonpath -%>
pythonpath = <%= @pythonpath %>
<% end -%>
<% if @wsgi_file -%>
wsgi-file = <%= @wsgi_file %>
<% end -%>
touch-reload = <%= @touch_reload_file %>
<% if @virtualenv -%>
virtualenv = <%= @virtualenv %>
<% end -%>
<% if @chdir -%>
chdir = <%= @chdir %>
<% end -%>
